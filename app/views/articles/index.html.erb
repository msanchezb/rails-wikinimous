<div class="wrapper">
  <div class="article-container">
    <ul class="text-center">
      <% @articles.each do |article| %>
        <li class="article">
          <h3>
            <%= link_to article.title, article_path(article) %>
          </h3>
          <p><%= article.content %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
